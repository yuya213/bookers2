<% if @book.errors.any? %>
  <%= @book.errors.count %>件のerrorが発生しました
  <ul>
    <% @book.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>

  <div class="container px-5 px-sm-0">
  <div class="row">
   
   

<%= render "list", user: @user, book: @book %>
  

<%= render "index",  books: @books %>

</div>
</div>
  

